{
  "name": "@meetup/now-client",
  "version": "0.0.1",
  "description": "Now Apollo Client",
  "main": "src/index.js",
  "keywords": [
    "apollo",
    "client"
  ],
  "scripts": {
    "build": "rm -rf build; webpack",
    "test": "yarn lint:prettier && eslint . && jest --coverage && flow check",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "precommit": "lint-staged && yarn test",
    "lint": "eslint .",
    "lint:prettier": "prettier -l 'src/**/*.js'",
    "lint:prettier:fix": "prettier --write 'src/**/*.js'"
  },
  "author": "Andy Stanberry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meetup/now-client/issues"
  },
  "homepage": "https://github.com/meetup/now-client",
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.3.1",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.0.9",
    "apollo-link-logger": "^1.2.2",
    "apollo-link-retry": "^2.2.3",
    "apollo-link-state": "^0.4.1",
    "apollo-link-token-refresh": "^0.2.3",
    "apollo-link-ws": "^1.0.8",
    "apollo-upload-client": "^8.0.0",
    "apollo-utilities": "^1.0.12",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "subscriptions-transport-ws": "^0.9.11"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jest": "21.15.1",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "flow-bin": "^0.73.0",
    "husky": "^0.14.3",
    "jest": "23.0.1",
    "lint-staged": "^7.1.2",
    "prettier": "^1.13.2",
    "webpack": "^4.10.1",
    "webpack-cli": "^2.1.4"
  },
  "resolutions": {
    "eslint-scope": "3.7.1"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/helpers/*.js"
    ],
    "collectCoverage": true
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  }
}
